{
    "name": "simialbi/yii2-widget-dropzone",
    "description": "Yii2 Dropzone widget",
    "type": "yii2-extension",
    "keywords": [
        "yii2",
        "dropzone",
        "upload"
    ],
    "license": "GPL-3.0+",
    "authors": [
        {
            "name": "Alexander Kozhevnikov",
            "email": "b37hr3z3n@gmail.com"
        },
        {
            "name": "Evgeny Dubovitsky",
            "email": "flynn068@gmail.com"
        },
        {
            "name": "Pavel Fedotov",
            "email": "fps.06@mail.ru"
        },
        {
            "name": "Simon Karlen",
            "email": "simi.albi@outlook.com",
            "homepage": "https://github.com/simialbi"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4",
        "yiisoft/yii2": "^2.0.0",
        "bower-asset/dropzone": "^5.0.0"
    },
    "require-dev": {
        "yiisoft/yii2-coding-standards": "^2.0.0",
        "yiisoft/yii2-debug": "2.0.*"
    },
    "autoload": {
        "psr-4": {
            "simialbi\\yii2\\dropzone\\": "src"
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
